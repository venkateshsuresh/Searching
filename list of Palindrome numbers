#include<stdio.h>
    int main() {
        int r;
        scanf("%d", &r);
        for(int i=0;i<=r;i++){
                if(i>=0 && i<=9){
                    printf("%d ",i);
                }
                if(i>=10 || i>=100){
                    if((i/10)==(i%10)){
                            printf("%d ",i);
                    }
                }
                if(i>100 || i>=1000){
                    if((i/100)==(i%10)){
                            printf("%d ",i);
                    }
                }
                if(i>1000 || i>=10000){
                    if(((i/1000)==(i%10))&&(((i/100)%10)==(((i%100)/10)))){
                            printf("%d ",i);
                    }
                }
        }
        return 0;
    }
